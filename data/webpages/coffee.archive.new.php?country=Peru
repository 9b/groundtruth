		   opener_id.innerHTML = '+';		   createCookie(theid, 'none', 1);        }    }}function menu_cooks() {	show_cook('sub_green_coffee');	show_cook('sub_central');	show_cook('sub_south');	show_cook('sub_africa');	show_cook('sub_asia');	show_cook('sub_islands');	show_cook('sub_products');	show_cook('sub_grinders');	show_cook('sub_roasters');	show_cook('sub_library');	show_cook('define_container');	show_cook('definebox');	show_cook('define_body');	show_cook('gloss_min');	show_cook('sub_ordering');	show_cook('sub_glossary');	show_cook('sub_roasted');	show_cook('sub_search');}// not for sidebar: function to flip the review to display the spider graphfunction show_spider(ident) {	var rev = ident+'rev';	var img = ident+'img';	var imgtext = ident+'spider';	var revtext = ident+'review';	var imgtext3 = ident+'spider2';	var revtext3 = ident+'review2';	var image = document.getElementById(img);	var imgtext2 = document.getElementById(imgtext);	var revtext2 = document.getElementById(revtext);	var imgtext4 = document.getElementById(imgtext3);	var revtext4 = document.getElementById(revtext3);		image.style.display = 'block';	imgtext2.style.display = 'none';	revtext2.style.display = 'block';	imgtext4.style.display = 'none';	revtext4.style.display = 'block';}function show_review(ident) {	var img = ident+'img';	var imgtext = ident+'spider';	var revtext = ident+'review';	var imgtext3 = ident+'spider2';	var revtext3 = ident+'review2';		var image = document.getElementById(img);	var imgtext2 = document.getElementById(imgtext);	var revtext2 = document.getElementById(revtext);	var imgtext4 = document.getElementById(imgtext3);	var revtext4 = document.getElementById(revtext3);		image.style.display = 'none';	imgtext2.style.display = 'block';	revtext2.style.display = 'none';	imgtext4.style.display = 'block';	revtext4.style.display = 'none';}function min_dict() {	var switch_id = document.getElementById('define_body');	var x = document.getElementById('gloss_min');	if (switch_id.style.display != 'none') {		switch_id.style.display = 'none';		createCookie('define_body', 'none', 0);				x.style.display = 'none';		createCookie('gloss_min', 'none', 0);	}	}function max_dict() {	var switch_id = document.getElementById('define_body');	var x = document.getElementById('gloss_min');	if (switch_id.style.display == 'none') {		switch_id.style.display = 'block';		createCookie('define_body', 'block', 0);				x.style.display = 'inline';		createCookie('gloss_min', 'inline', 0);	} 	}function show_dict() {	var switch_id = document.getElementById('definebox');	var dc = document.getElementById('define_container');	if (switch_id.style.display == 'none') {		switch_id.style.display = 'block';		createCookie('definebox', 'block', 0);		define_container.style.display = 'block';		createCookie('define_container', 'block', 0);	} else {		switch_id.style.display = 'none';		createCookie('definebox', 'none', 0);	}	}//--></script><script language="javascript">names = new Array();images = new Array();urls = new Array();    names.push("Kenya Kirinyaga Kiangoi AA ");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/kiangoi.jpg' width='175' height='263' alt='' />");urls.push("http://www.sweetmarias.com/coffee.africa.kenya.php#4285");names.push("Ethiopia Dry Process Gr. 1 Yirga Cheffe Aricha");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/Aricha%20Private%20Mill%2C%20Idido%2C%20Yirga%20Cheffe%2058.jpg' width='175' height='263' alt='' />");urls.push("http://www.sweetmarias.com/coffee.africa.ethiopia.php#4246");names.push("Ethiopia Deri Kochowa -Kifle Station");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/F