<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"><html xmlns="http://www.w3.org/1999/xhtml"><head><meta http-equiv="Content-Type" content="text/html; charset=UTF-8" /><meta name="description" content="Archive of coffee reviews by Sweet Maria's" /> <title>Sweet Maria's Coffee Archives 2009-</title><link href="new_style.css" rel="stylesheet" type="text/css" /><script language="JavaScript" type="text/JavaScript"><!--// cookie functions from quirksmodefunction createCookie(name,value,days) {	if (days) {		var date = new Date();		date.setTime(date.getTime()+(days*24*60*60*1000));		var expires = "; expires="+date.toGMTString();	}	else var expires = "";	document.cookie = name+"="+value+expires+"; path=/";}function readCookie(name) {	var nameEQ = name + "=";	var ca = document.cookie.split(';');	for(var i=0;i < ca.length;i++) {		var c = ca[i];		while (c.charAt(0)==' ') c = c.substring(1,c.length);		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);	}	return null;}function eraseCookie(name) {	createCookie(name,"",-1);}// function to keep sidebar menu state based on cookiesfunction show_cook(name) {	var x = readCookie(name);	if (x != null) {		var opener_txt = name+'_open';		var y = document.getElementById(name);		y.style.display = x;		var z;		if (z = document.getElementById(opener_txt)) {			z.innerHTML = (x == 'none') ? '+' : '-';		}	}}function showHide_sb(theid){    if (document.getElementById) {    	var opener_txt = theid+'_open';		var switch_id = document.getElementById(theid);		var opener_id = document.getElementById(opener_txt);				// if currently invisible        if(switch_id.style.display == 'none') {			// make this visible 	       	switch_id.style.display = 'block';			opener_id.innerHTML = '-';			createCookie(theid, 'block', 1);				//otherwise, we're hiding the thing		} else{		   switch_id.style.display = 'none';		   opener_id.innerHTML = '+';		   createCookie(theid, 'none', 1);        }    }}function menu_cooks() {	show_cook('sub_green_coffee');	show_cook('sub_central');	show_cook('sub_south');	show_cook('sub_africa');	show_cook('sub_asia');	show_cook('sub_islands');	show_cook('sub_products');	show_cook('sub_grinders');	show_cook('sub_roasters');	show_cook('sub_library');	show_cook('define_container');	show_cook('definebox');	show_cook('define_body');	show_cook('gloss_min');	show_cook('sub_ordering');	show_cook('sub_glossary');	show_cook('sub_roasted');	show_cook('sub_search');}// not for sidebar: function to flip the review to display the spider graphfunction show_spider(ident) {	var rev = ident+'rev';	var img = ident+'img';	var imgtext = ident+'spider';	var revtext = ident+'review';	var imgtext3 = ident+'spider2';	var revtext3 = ident+'review2';	var image = document.getElementById(img);	var imgtext2 = document.getElementById(imgtext);	var revtext2 = document.getElementById(revtext);	var imgtext4 = document.getElementById(imgtext3);	var revtext4 = document.getElementById(revtext3);		image.style.display = 'block';	imgtext2.style.display = 'none';	revtext2.style.display = 'block';	imgtext4.style.display = 'none';	revtext4.style.display = 'block';}function show_review(ident) {	var img = ident+'img';	var imgtext = ident+'spider';	var revtext = ident+'review';	var imgtext3 = ident+'spider2';	var revtext3 = ident+'review2';		var image = document.getElementById(img);	var imgtext2 = document.getElementById(imgtext);	var revtext2 = document.getElementById(revtext);	var imgtext4 = document.getElementById(imgtext3);	var revtext4 = document.getElementById(revtext3);		image.style.display = 'none';	imgtext2.style.display = 'block';	revtext2.style.display = 'none';	imgtext4.style.display = 'block';	revtext4.style.display = 'none';}function min_dict() {	var switch_id = document.getElementById('define_body');	var x = document.getElementById('gloss_min');	if (switch_id.style.display != 'none') {		switch_id.style.display = 'none';		createCookie('define_body', 'none', 0);				x.style.display = 'none';		createCookie('gloss_min', 'none', 0);	}	}function max_dict() {	var switch_id = document.getElementById('define_body');	var x = document.getElementById('gloss_min');	if (switch_id.style.display == 'none') {		switch_id.style.display = 'block';		createCookie('define_body', 'block', 0);				x.style.display = 'inline';		createCookie('gloss_min', 'inline', 0);	} 	}function show_dict() {	var switch_id = document.getElementById('definebox');	var dc = document.getElementById('define_container');	if (switch_id.style.display == 'none') {		switch_id.style.display = 'block';		createCookie('definebox', 'block', 0);		define_container.style.display = 'block';		createCookie('define_container', 'block', 0);	} else {		switch_id.style.display = 'none';		createCookie('definebox', 'none', 0);	}	}//--></script><script language="javascript">names = new Array();images = new Array();urls = new Array();    names.push("Kenya Kirinyaga Kiangoi AA ");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/kiangoi.jpg' width='175' height='263' alt='' />");urls.push("http://www.sweetmarias.com/coffee.africa.kenya.php#4285");names.push("Ethiopia Dry Process Gr. 1 Yirga Cheffe Aricha");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/Aricha%20Private%20Mill%2C%20Idido%2C%20Yirga%20Cheffe%2058.jpg' width='175' height='263' alt='' />");urls.push("http://www.sweetmarias.com/coffee.africa.ethiopia.php#4246");names.push("Ethiopia Deri Kochowa -Kifle Station");images.push("<img typeof='foaf:Image' src='http://www.sweetmarias.com/coffee/sites/default/files/styles/sm_sidebar/public/coffee-review-phoots/F